<%= form_tag coupons_path do %>
<label>Coupon Form</label><br>

<p> Code: <%= text_area_tag :"coupon[coupon_code]" %><br></p> <br>
<p> Store: <%= text_area_tag :"coupon[store]" %><br></p> <br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag "Submit Coupon" %>
<% end %>

